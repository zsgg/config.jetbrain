<application>
  <component name="DefaultFont">
    <option name="FONT_FAMILY" value="Hack" />
  </component>
  <component name="EditorSettings">
    <option name="USE_SOFT_WRAPS" value="CONSOLE" />
  </component>
  <component name="TodoConfiguration">
    <pattern pattern="\btodo\b.*" />
    <pattern pattern="\bfixme\b.*" />
    <pattern icon="question" useCustomColors="true" pattern="\b(?&lt;!@author )gssong\b.*">
      <option name="FOREGROUND" value="ffffff" />
      <option name="BACKGROUND" value="181a1f" />
      <option name="FONT_TYPE" value="2" />
      <option name="EFFECT_COLOR" value="181a1f" />
    </pattern>
    <pattern icon="question" useCustomColors="true" pattern="\bzsgg\b.*">
      <option name="FOREGROUND" value="ffffff" />
      <option name="BACKGROUND" value="181a1f" />
      <option name="FONT_TYPE" value="2" />
      <option name="EFFECT_COLOR" value="181a1f" />
      <option name="ERROR_STRIPE_COLOR" value="181a1f" />
      <option name="EFFECT_TYPE" value="1" />
    </pattern>
    <pattern icon="question" pattern="\b(?&lt;!@author )gssong v \b.*" />
    <filter name="gssong">
      <pattern index="2" />
    </filter>
    <filter name="gssong v">
      <pattern index="4" />
    </filter>
  </component>
</application>