<keymap version="1" name="Default copy" parent="$default">
  <action id="CloseAllUnpinnedEditors">
    <keyboard-shortcut first-keystroke="alt e" />
  </action>
  <action id="CloseContent">
    <keyboard-shortcut first-keystroke="alt w" />
  </action>
  <action id="GotoNextBookmark">
    <keyboard-shortcut first-keystroke="alt c" />
  </action>
  <action id="GotoPreviousBookmark">
    <keyboard-shortcut first-keystroke="alt z" />
  </action>
  <action id="MoveTabRight">
    <keyboard-shortcut first-keystroke="alt quote" />
  </action>
  <action id="NextSplitter">
    <keyboard-shortcut first-keystroke="alt close_bracket" />
  </action>
  <action id="NextTab">
    <keyboard-shortcut first-keystroke="alt d" />
  </action>
  <action id="PrevSplitter">
    <keyboard-shortcut first-keystroke="alt open_bracket" />
  </action>
  <action id="PreviousTab">
    <keyboard-shortcut first-keystroke="alt a" />
  </action>
  <action id="ReopenClosedTab">
    <keyboard-shortcut first-keystroke="alt t" />
  </action>
  <action id="ShowBookmarks">
    <keyboard-shortcut first-keystroke="shift f11" />
    <keyboard-shortcut first-keystroke="alt v" />
  </action>
  <action id="ToggleBookmark">
    <keyboard-shortcut first-keystroke="alt x" />
  </action>
  <action id="Unsplit">
    <keyboard-shortcut first-keystroke="alt semicolon" />
  </action>
</keymap>